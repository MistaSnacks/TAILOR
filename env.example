# Google Gemini AI
GEMINI_API_KEY=your_gemini_api_key

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Job Search APIs (JSearch via RapidAPI)
JSEARCH_ENABLED=false
JSEARCH_API_KEY=your_rapidapi_key_here
JSEARCH_HOST=jsearch.p.rapidapi.com
# Job Search APIs (Dev Only)
# LinkedIn RapidAPI - ONLY enable in development, not production-safe
LINKEDIN_RAPID_ENABLED=false
LINKEDIN_RAPID_API_KEY=your_rapidapi_key_here
LINKEDIN_RAPID_HOST=linkedin-job-search-api.p.rapidapi.com
